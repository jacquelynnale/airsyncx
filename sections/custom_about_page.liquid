<!-- AirSyncX About Us Page -->
<!-- Google Fonts - Assistant -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Assistant:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">

<!-- AOS Animation Library -->
<link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
<script src="https://unpkg.com/aos@next/dist/aos.js"></script>

<div class="airsyncx-about-page">
  <!-- Animated Background -->
  <div class="animated-background">
    <div class="animated-particles"></div>
    <div class="animated-lines"></div>
    <div class="animated-gradient"></div>
  </div>
  
  <!-- About header -->
  <div class="page-width">
    <div class="about-header" data-aos="fade-up" data-aos-duration="1000">
      <h1 class="about-heading">אודות <span class="highlight">AirSyncX</span></h1>
      <p class="about-subheading">{{ section.settings.subheading }}</p>
    </div>

    <!-- About Main Content -->
    <div class="about-main-content">
      <div class="about-story-container" data-aos="fade-up" data-aos-delay="200">
        <h3 class="about-story-title"><span class="white-text">הסיפור</span> <span class="highlight">שלנו</span></h3>
        <div class="about-story-content">
          <p>{{ section.settings.story_text }}</p>
        </div>
      </div>

      <div class="about-vision-container" data-aos="fade-up" data-aos-delay="300">
        <h3 class="about-vision-title"><span class="white-text">החזון</span> <span class="highlight">שלנו</span></h3>
        <div class="about-vision-content">
          <p>{{ section.settings.vision_text }}</p>
        </div>
      </div>
    </div>

    <!-- Team Members Section -->
    <div class="team-section" data-aos="fade-up" data-aos-delay="400">
      <h2 class="team-heading">הצוות <span class="highlight">שלנו</span></h2>
      <p class="team-subheading">{{ section.settings.team_description }}</p>
      
      <div class="team-members">
        {%- for block in section.blocks -%}
          {%- if block.type == 'team_member' -%}
            <div class="team-member" data-aos="fade-up" data-aos-delay="{{ forloop.index | times: 100 }}">
              <div class="member-image-container">
                <img src="{{ block.settings.member_image | img_url: 'master' }}" alt="{{ block.settings.member_name }}" class="member-image">
                <div class="member-image-glow"></div>
              </div>
              <h3 class="member-name">{{ block.settings.member_name }}</h3>
              <p class="member-position">{{ block.settings.member_position }}</p>
              <p class="member-bio">{{ block.settings.member_bio }}</p>
            </div>
          {%- endif -%}
        {%- endfor -%}
      </div>
    </div>

    <!-- Values Section -->
    <div class="values-section" data-aos="fade-up">
      <h2 class="values-heading">הערכים <span class="highlight">שלנו</span></h2>
      <p class="values-subheading">{{ section.settings.values_description }}</p>
      
      <div class="values-grid">
        {%- for block in section.blocks -%}
          {%- if block.type == 'value_item' -%}
            <div class="value-item" data-aos="fade-up" data-aos-delay="{{ forloop.index | times: 100 }}">
              <div class="value-icon-container">
                <div class="value-icon">
                  {% case block.settings.value_icon %}
                    {% when 'innovation' %}
                      <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M12 2C6.48 2 2 6.48 2 12C2 17.52 6.48 22 12 22C17.52 22 22 17.52 22 12" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M22 2L13 11" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M16 2H22V8" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                    {% when 'quality' %}
                      <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M12 15C15.866 15 19 11.866 19 8C19 4.13401 15.866 1 12 1C8.13401 1 5 4.13401 5 8C5 11.866 8.13401 15 12 15Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M8.21 13.89L7 23L12 20L17 23L15.79 13.88" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                    {% when 'customer' %}
                      <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M17 21V19C17 17.9391 16.5786 16.9217 15.8284 16.1716C15.0783 15.4214 14.0609 15 13 15H5C3.93913 15 2.92172 15.4214 2.17157 16.1716C1.42143 16.9217 1 17.9391 1 19V21" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M9 11C11.2091 11 13 9.20914 13 7C13 4.79086 11.2091 3 9 3C6.79086 3 5 4.79086 5 7C5 9.20914 6.79086 11 9 11Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M23 21V19C22.9993 18.1137 22.7044 17.2528 22.1614 16.5523C21.6184 15.8519 20.8581 15.3516 20 15.13" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M16 3.13C16.8604 3.35031 17.623 3.85071 18.1676 4.55232C18.7122 5.25392 19.0078 6.11683 19.0078 7.005C19.0078 7.89318 18.7122 8.75608 18.1676 9.45769C17.623 10.1593 16.8604 10.6597 16 10.88" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                    {% when 'sustainability' %}
                      <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M12 22C17.5228 22 22 17.5228 22 12C22 6.47715 17.5228 2 12 2C6.47715 2 2 6.47715 2 12C2 17.5228 6.47715 22 12 22Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M3 15L12 8L21 15" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M7 17L12 13.5L17 17" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                    {% else %}
                      <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M5 3V7M3 5H7M6 17V21M4 19H8M13 3L15.2857 9.85714L21 12L15.2857 14.1429L13 21L10.7143 14.1429L5 12L10.7143 9.85714L13 3Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                      </svg>
                  {% endcase %}
                </div>
              </div>
              <h3 class="value-title">{{ block.settings.value_title }}</h3>
              <p class="value-text">{{ block.settings.value_text }}</p>
            </div>
          {%- endif -%}
        {%- endfor -%}
      </div>
    </div>

    <!-- CTA Container -->
    <div class="cta-container" data-aos="fade-up">
      <div class="cta-content">
        <h3 class="cta-title"><span class="white-text">רוצים</span> <span class="highlight">לשמוע עוד?</span></h3>
        <p class="cta-text">{{ section.settings.cta_text }}</p>
        
        <div class="cta-buttons">
          <a href="{{ section.settings.cta_button_link }}" class="hero-button hero-button-primary">
            <span class="button-text">{{ section.settings.cta_button_text }}</span>
            <span class="button-icon">
              <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M12 4L10.59 5.41L16.17 11H4V13H16.17L10.59 18.59L12 20L20 12L12 4Z" fill="currentColor"/>
              </svg>
            </span>
          </a>
        </div>
      </div>
    </div>
  </div>
</div>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    AOS.init({
      once: true,
      disable: 'phone',
      duration: 1000
    });
    
    // Create and animate particles
    const particlesContainer = document.querySelector('.animated-particles');
    if (particlesContainer) {
      for (let i = 0; i < 50; i++) {
        let particle = document.createElement('div');
        particle.classList.add('particle');
        
        // Random position
        let posX = Math.random() * 100;
        let posY = Math.random() * 100;
        
        // Random size
        let size = Math.random() * 5 + 1;
        
        // Random opacity
        let opacity = Math.random() * 0.5 + 0.1;
        
        // Random animation delay
        let delay = Math.random() * 5;
        
        // Apply styles
        particle.style.left = `${posX}%`;
        particle.style.top = `${posY}%`;
        particle.style.width = `${size}px`;
        particle.style.height = `${size}px`;
        particle.style.opacity = opacity;
        particle.style.animationDelay = `${delay}s`;
        
        particlesContainer.appendChild(particle);
      }
    }
  });
</script>

<style>
  :root {
    --page-background: #000000;
  }
  
  body {
    background-color: var(--page-background);
    margin: 0;
    padding: 0;
  }
  
  .airsyncx-about-page {
    background-color: var(--page-background);
    color: #ffffff;
    position: relative;
    font-family: 'Assistant', sans-serif;
    overflow: hidden;
    padding-bottom: 0;
    margin-bottom: 0;
    direction: rtl;
    text-align: right;
  }
  
  .page-width {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 2rem;
  }
  
  /* Animated Background */
  .animated-background {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 0;
    background-color: var(--page-background);
    overflow: hidden;
  }
  
  /* Background Elements */
  .animated-particles {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
  
  .particle {
    position: absolute;
    background-color: #38d2d2;
    border-radius: 50%;
    animation: float 15s infinite ease-in-out;
  }
  
  .animated-lines {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-image: linear-gradient(rgba(56, 210, 210, 0.05) 1px, transparent 1px),
                      linear-gradient(90deg, rgba(56, 210, 210, 0.05) 1px, transparent 1px);
    background-size: 50px 50px;
    opacity: 0.2;
  }
  
  .animated-gradient {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: radial-gradient(circle at top right, rgba(56, 210, 210, 0.1), transparent 60%),
                radial-gradient(circle at bottom left, rgba(56, 210, 210, 0.1), transparent 60%);
    mix-blend-mode: screen;
  }
  
  /* Header Styling */
  .about-header {
    text-align: center;
    padding: 5rem 0 3rem;
    position: relative;
    z-index: 1;
  }
  
  .about-heading {
    font-size: 6rem;
    font-weight: 800;
    margin-bottom: 30px;
    color: #ffffff;
    position: relative;
    display: inline-block;
    letter-spacing: -0.5px;
  }
  
  .highlight {
    color: #38d2d2;
  }
  
  .about-subheading {
    font-size: 1.8rem;
    max-width: 800px;
    margin: 0 auto;
    color: rgba(255, 255, 255, 0.85);
    line-height: 1.5;
    font-weight: 400;
  }
  
  /* Main Content */
  .about-main-content {
    display: flex;
    flex-wrap: wrap;
    gap: 2rem;
    margin-bottom: 5rem;
    position: relative;
    z-index: 1;
  }
  
  .about-story-container,
  .about-vision-container {
    background-color: rgba(17, 17, 17, 0.6);
    padding: 2.5rem;
    border-radius: 12px;
    flex: 1 1 400px;
    backdrop-filter: blur(10px);
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
    border: 1px solid rgba(56, 210, 210, 0.1);
  }
  
  .about-story-title,
  .about-vision-title {
    font-size: 3rem;
    margin-bottom: 2rem;
    font-weight: 700;
    line-height: 1.2;
  }
  
  .white-text {
    color: #ffffff;
  }
  
  .about-story-content,
  .about-vision-content {
    color: rgba(255, 255, 255, 0.9);
    font-size: 1.2rem;
    line-height: 1.8;
  }
  
  /* Team Section */
  .team-section {
    margin-bottom: 6rem;
    position: relative;
    z-index: 1;
  }
  
  .team-heading {
    font-size: 6rem;
    font-weight: 800;
    margin-bottom: 30px;
    color: #ffffff;
    position: relative;
    display: inline-block;
    letter-spacing: -0.5px;
    text-align: center;
    width: 100%;
  }
  
  .team-subheading {
    font-size: 1.8rem;
    max-width: 800px;
    margin: 0 auto 4rem;
    color: rgba(255, 255, 255, 0.85);
    line-height: 1.5;
    font-weight: 400;
    text-align: center;
  }
  
  .team-members {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 3rem;
    margin-top: 3rem;
  }
  
  .team-member {
    flex: 0 0 calc(33.333% - 2rem);
    min-width: 280px;
    text-align: center;
    background-color: rgba(17, 17, 17, 0.4);
    border-radius: 12px;
    padding: 2rem;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
    border: 1px solid rgba(56, 210, 210, 0.1);
    transition: all 0.3s ease;
  }
  
  .team-member:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.4);
    border-color: rgba(56, 210, 210, 0.3);
  }
  
  .member-image-container {
    position: relative;
    width: 180px;
    height: 180px;
    margin: 0 auto 2rem;
    border-radius: 50%;
    overflow: hidden;
  }
  
  .member-image {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: 50%;
    transition: transform 0.5s ease;
  }
  
  .team-member:hover .member-image {
    transform: scale(1.05);
  }
  
  .member-image-glow {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    box-shadow: inset 0 0 20px rgba(56, 210, 210, 0.5);
    opacity: 0;
    transition: opacity 0.3s ease;
  }
  
  .team-member:hover .member-image-glow {
    opacity: 1;
  }
  
  .member-name {
    font-size: 1.8rem;
    font-weight: 700;
    margin-bottom: 0.5rem;
    color: #ffffff;
  }
  
  .member-position {
    font-size: 1.2rem;
    color: #38d2d2;
    margin-bottom: 1.5rem;
    font-weight: 500;
  }
  
  .member-bio {
    font-size: 1rem;
    line-height: 1.6;
    color: rgba(255, 255, 255, 0.8);
  }
  
  /* Values Section */
  .values-section {
    margin-bottom: 6rem;
    position: relative;
    z-index: 1;
  }
  
  .values-heading {
    font-size: 6rem;
    font-weight: 800;
    margin-bottom: 30px;
    color: #ffffff;
    position: relative;
    display: inline-block;
    letter-spacing: -0.5px;
    text-align: center;
    width: 100%;
  }
  
  .values-subheading {
    font-size: 1.8rem;
    max-width: 800px;
    margin: 0 auto 4rem;
    color: rgba(255, 255, 255, 0.85);
    line-height: 1.5;
    font-weight: 400;
    text-align: center;
  }
  
  .values-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
    gap: 2rem;
  }
  
  .value-item {
    background-color: rgba(17, 17, 17, 0.4);
    border-radius: 12px;
    padding: 2rem;
    text-align: center;
    box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
    border: 1px solid rgba(56, 210, 210, 0.1);
    transition: all 0.3s ease;
  }
  
  .value-item:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.4);
    border-color: rgba(56, 210, 210, 0.3);
  }
  
  .value-icon-container {
    background-color: rgba(56, 210, 210, 0.1);
    width: 80px;
    height: 80px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto 1.5rem;
    transition: all 0.3s ease;
  }
  
  .value-item:hover .value-icon-container {
    background-color: rgba(56, 210, 210, 0.2);
    transform: scale(1.1);
  }
  
  .value-icon {
    width: 40px;
    height: 40px;
    color: #38d2d2;
  }
  
  .value-title {
    font-size: 1.8rem;
    font-weight: 700;
    margin-bottom: 1rem;
    color: #ffffff;
  }
  
  .value-text {
    font-size: 1.1rem;
    line-height: 1.6;
    color: rgba(255, 255, 255, 0.8);
  }
  
  /* CTA Container */
  .cta-container {
    background-color: rgba(17, 17, 17, 0.6);
    border-radius: 16px;
    padding: 3.5rem;
    margin: 6rem 0 0 0;
    position: relative;
    z-index: 1;
    text-align: center;
    backdrop-filter: blur(10px);
    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.4);
    border: 1px solid rgba(56, 210, 210, 0.15);
    overflow: hidden;
  }
  
  .cta-container::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: radial-gradient(circle at top right, rgba(56, 210, 210, 0.15), transparent 60%);
    z-index: -1;
  }
  
  .cta-content {
    position: relative;
    z-index: 2;
  }
  
  .cta-title {
    font-size: 4rem;
    font-weight: 700;
    margin-bottom: 1.5rem;
    line-height: 1.2;
  }
  
  .cta-text {
    font-size: 1.8rem;
    color: rgba(255, 255, 255, 0.85);
    max-width: 800px;
    margin: 0 auto 2.5rem;
    line-height: 1.5;
  }
  
  .cta-buttons {
    display: flex;
    gap: 1.5rem;
    justify-content: center;
  }
  
  .hero-button {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 10px;
    background-color: #38d2d2;
    color: #000000;
    border: none;
    padding: 1.2rem 2.5rem;
    border-radius: 10px;
    font-size: 1.4rem;
    font-weight: 600;
    cursor: pointer;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
    font-family: 'Assistant', sans-serif;
  }
  
  .hero-button:hover {
    background-color: #2ab8b8;
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(56, 210, 210, 0.3);
  }
  
  .hero-button-primary {
    background-color: #38d2d2;
    color: #000000;
  }
  
  .hero-button-primary:hover {
    background-color: #2ab8b8;
  }
  
  .button-icon {
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .button-text {
    position: relative;
    z-index: 2;
    font-size: 1.4rem;
  }
  
  /* Animations */
  @keyframes float {
    0%, 100% {
      transform: translateY(0) translateX(0);
    }
    25% {
      transform: translateY(-20px) translateX(10px);
    }
    50% {
      transform: translateY(0) translateX(20px);
    }
    75% {
      transform: translateY(20px) translateX(10px);
    }
  }
  
  @keyframes pulse {
    0%, 100% {
      opacity: 0.2;
    }
    50% {
      opacity: 0.5;
    }
  }
  
  /* Responsive Styles */
  @media screen and (max-width: 768px) {
    .about-heading,
    .team-heading,
    .values-heading {
      font-size: 4rem;
    }
    
    .about-subheading,
    .team-subheading,
    .values-subheading {
      font-size: 1.4rem;
    }
    
    .about-story-title,
    .about-vision-title {
      font-size: 2.5rem;
    }
    
    .cta-title {
      font-size: 3rem;
    }
    
    .cta-text {
      font-size: 1.4rem;
    }
    
    .cta-container {
      padding: 2.5rem 1.5rem;
    }
    
    .team-member {
      flex: 0 0 100%;
    }
  }
</style>

{% schema %}
{
  "name": "Custom About Page",
  "settings": [
    {
      "type": "text",
      "id": "subheading",
      "label": "Subheading",
      "default": "הסיפור של המוצר המהפכני שמשנה את חווית הנהיגה שלכם לטובה"
    },
    {
      "type": "textarea",
      "id": "story_text",
      "label": "Story Text",
      "default": "AirSyncX החל את דרכו בשנת 2023 כאשר צוות של מהנדסים מנוסים מתחום הרכב והטכנולוגיה זיהו פער משמעותי בעולם החיבורים האלחוטיים ברכב. לאחר שנתיים של פיתוח וניסויים, השקנו את המוצר הראשון שלנו אשר מאפשר לכל נהג ליהנות מחוויית CarPlay אלחוטית, גם ברכבים שתומכים רק בחיבור קווי. המשימה שלנו היא להפוך את הטכנולוגיה המתקדמת לנגישה ופשוטה לשימוש עבור כל נהג."
    },
    {
      "type": "textarea",
